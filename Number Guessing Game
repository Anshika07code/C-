#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int num, guess;

    srand(time(0));
    num = rand() % 10 + 1;

    printf("Guess a number (1 to 10): ");
    scanf("%d", &guess);

    if (guess == num)
        printf("Correct! ğŸ‰");
    else
        printf("Wrong! Number was %d", num);

    return 0;
}
