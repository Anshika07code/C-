using System;

class Student
{
    public string name;
    public int age;

    public void Display()
    {
        Console.WriteLine(name + " - " + age);
    }
}

class Program
{
    static void Main()
    {
        Student s = new Student();
        s.name = "Anshika";
        s.age = 20;
        s.Display();
    }
}
